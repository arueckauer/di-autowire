{
    "name": "arueckauer/di-autowire",
    "type": "project",
    "description": "opsway/laminas-service-manager-attributes tests",
    "homepage": "https://github.com/arueckauer/di-autowire",
    "license": "BSD-3-Clause",
    "require": {
        "php": "~8.1.0",
        "laminas/laminas-config-aggregator": "^1.11",
        "laminas/laminas-servicemanager": "^3.19",
        "opsway/laminas-service-manager-attributes": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.4",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "laminas": {
            "component-whitelist": [
                "laminas/laminas-httphandlerrunner"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "scripts": {}
}
